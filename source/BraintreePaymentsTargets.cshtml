<?xml version="1.0" encoding="utf-8"?>
@using System.Linq
@using System.IO
@using System.Xml.Linq
<Project>
  <PropertyGroup>
    <_@(Model.NormalizedName)Id>@(Model.NormalizedName)</_@(Model.NormalizedName)Id>
  </PropertyGroup>
  <ItemGroup Condition="('$(OutputType)' != 'Library' OR '$(IsAppExtension)' == 'True') AND ('$(TargetFrameworks)' == '' AND '$(TargetFramework)' == '')">
    <_NativeReference Include="$(MSBuildThisFileDirectory)..\lib\xamarinios10\@(Model.NuGetPackageId).resources\@(Model.NormalizedName).xcframework">
        <_Id>$(_@(Model.NormalizedName)Id)</_Id>
    </_NativeReference>
  </ItemGroup>
  <Target Name="_ResolveNativeReferencesFor@(Model.NormalizedName)" 
          BeforeTargets="ResolveNativeReferences"
          Condition="('$(TargetFrameworks)' == '' AND '$(TargetFramework)' == '') OR '$(IsBindingProject)' == 'true'">
    <ItemGroup>
      <_NativeReference Update="@@(_NativeReference)" Condition="'%(_NativeReference._Id)' == '$(_@(Model.NormalizedName)Id)'">
        <Kind>Framework</Kind>
        <SmartLink>True</SmartLink>
        <ForceLoad>True</ForceLoad>
        <LinkerFlags>-ObjC -lz</LinkerFlags>
        <Frameworks>Security SystemConfiguration</Frameworks>
      </_NativeReference>
      <NativeReference Include="@@(_NativeReference)" />
    </ItemGroup>
  </Target>
</Project>